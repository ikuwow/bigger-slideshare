{
    "manifest_version": 2,
    "name": "Larger Slides",
    "version": "0.1.0",
    "content_scripts": [
        {
            "matches": ["https://www.slideshare.net/*"],
            "js": ["slideshare.js"]
        },
        {
            "matches": ["https://speakerdeck.com/*"],
            "js": ["speakerdeck.js"]
        }
    ]
}
